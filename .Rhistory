nburn=ngibbs/2
nparam=ncol(xmat)
nloc=nrow(dat)
nspp=ncol(dat)
#priors
phi.prior=0.1
lambda.a=lambda.b=0.1
betas=matrix(0,nparam,ncomm)
#initial values
array.lsk=array(0,dim=c(nloc,nspp,ncomm))
for (i in 1:nloc){
for (j in 1:nspp){
if (y[i,j]!=0){
array.lsk[i,j,]=rmultinom(1,size=y[i,j],prob=rep(1/ncomm,ncomm))
}
}
}
#basic test
